(defdomain sokoban (

  (:attachments
    (adjacent ?from ?to)
    (pushable ?from ?intermediate ?to)
  )

  (:operator (!move ?from ?to)
    ()
    ((clear ?to))
    ((clear ?from))
  )

  (:operator (!push ?from ?intermediate ?to)
    ()
    (
      (box ?intermediate)
      (clear ?to)
    )
    (
      (box ?to)
      (clear ?from)
    )
  )

  (:operator (!!visit ?player) (call new_state ?player) () ())

  (:method (solve ?player)
    no_box_outside_storage
    (call boxes_stored)
    ()
    push
    (
      (pushable ?player ?box ?clear)
      (not (deadlock ?clear))
    )
    (
      (!push ?player ?box ?clear)
      (!!visit ?box)
      (solve ?box)
    )
    move
    (
      (adjacent ?player ?clear)
      (call new_state ?clear)
    )
    (
      (!move ?player ?clear)
      (solve ?clear)
    )
  )
))